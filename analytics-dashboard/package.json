{
  "name": "analytics-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nohup node -r dotenv/config ./build/App.js &",
    "start-dev": "nodemon --exec babel-node -r node_modules/dotenv/config App.js",
    "start-cron-master-dev": "nodemon --exec babel-node -r node_modules/dotenv/config CronMaster.js",
    "start-cron-master": "nohup node -r dotenv/config ./build/CronMaster.js &",
    "start-cron-worker": "nohup node -r dotenv/config ./build/CronWorker.js &",
    "start-cron-worker-dev": "nodemon --exec babel-node -r node_modules/dotenv/config CronWorker.js",
    "start-cron-all": " npm run start-cron-worker && npm run start-cron-master",
    "start-all": "npm start && npm run start-cron-all",
    "stop": "node -r dotenv/config ./build/Stop.js",
    "stop-cron-master": "node -r dotenv/config ./build/StopCronMaster.js",
    "stop-cron-all": "npm run stop-cron-master && npm run stop-cron-worker",
    "stop-cron-worker": "node -r dotenv/config ./build/StopCronWorker.js",
    "stop-all": "npm run stop-cron-all && npm stop",
    "install-ui": "cd ui && npm install --force && cd ..",
    "install-server": "npm install --force",
    "install-all": "npm run install-ui && npm run install-server",
    "build-all": "npm run build-clean && npm run build-server && npm run build-ui && cross-env cpx ui/build/**/*.* build/ui/build",
    "build-server": "cross-env babel src --out-dir build/src --source-maps && cross-env babel *.js --out-dir build --source-maps",
    "build-ui": "cd ui && npm run build && cd ..",
    "build": "npm run install-all && npm run build-all && npm run tar-create && npm run tar-clean",
    "tar-create": "npm pack && cross-env cpx analytics-dashboard-1.0.0.tgz dist/",
    "build-clean": "rimraf build",
    "tar-clean": "rimraf analytics-dashboard-1.0.0.tgz",
    "test": "nodemon --exec babel-node -r node_modules/dotenv/config Test.js"
  },
  "dependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.5.5",
    "@babel/runtime": "7.9.2",
    "babel-eslint": "10.0.3",
    "bottleneck": "2.19.5",
    "change-case": "3.1.0",
    "cors": "2.8.5",
    "cpx": "1.5.0",
    "cross-env": "6.0.3",
    "cross-port-killer": "1.2.1",
    "dotenv": "8.0.0",
    "dotenv-safe": "6.1.0",
    "eslint": "6.1.0",
    "eslint-plugin-react": "7.17.0",
    "express": "4.17.3",
    "express-pino-logger": "4.0.0",
    "ioredis": "4.14.1",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "node-cache": "5.1.0",
    "node-cron": "2.0.3",
    "nodemailer": "6.3.1",
    "nodemailer-smtp-pool": "2.8.3",
    "nodemon": "1.19.1",
    "opencollective-postinstall": "2.0.2",
    "pino": "5.15.0",
    "pino-pretty": "3.5.0",
    "pm2": "4.2.3",
    "request": "2.88.0",
    "rimraf": "3.0.0",
    "socket.io-client": "2.3.0",
    "tar": "5.0.5",
    "underscore": "1.9.2"
  },
  "bundledDependencies": [
    "bottleneck",
    "change-case",
    "cross-port-killer",
    "cors",
    "dotenv",
    "dotenv-safe",
    "express",
    "moment",
    "morgan",
    "nodemailer",
    "nodemailer-smtp-pool",
    "request",
    "socket.io-client",
    "underscore",
    "pino",
    "express-pino-logger",
    "pino-pretty",
    "node-cache",
    "ioredis",
    "underscore",
    "opencollective-postinstall",
    "node-cron",
    "pm2"
  ]
}
